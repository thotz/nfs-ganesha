[project]
	description = NFS-Ganesha is an NFSv3,v4,v4.1 fileserver that runs in user mode on most UNIX/Linux systems
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Jiffin Tony Thottan (thotz)
[access "refs/for/refs/*"]
	read = group user/Jiffin Tony Thottan (thotz)
	push = group user/Jiffin Tony Thottan (thotz)
	label-Code-Review = -2..+2 group user/Jiffin Tony Thottan (thotz)
	label-Verified = -1..+1 group user/Jiffin Tony Thottan (thotz)
	rebase = group user/Jiffin Tony Thottan (thotz)
	removeReviewer = group user/Jiffin Tony Thottan (thotz)
	submit = group user/Jiffin Tony Thottan (thotz)
[access "refs/heads/*"]
	read = group user/Jiffin Tony Thottan (thotz)
	create = group user/Jiffin Tony Thottan (thotz)
	push = +force group user/Jiffin Tony Thottan (thotz)
	pushMerge = group user/Jiffin Tony Thottan (thotz)
[access "refs/tags/*"]
	push = +force group user/Jiffin Tony Thottan (thotz)
	pushTag = group user/Jiffin Tony Thottan (thotz)
	pushSignedTag = group user/Jiffin Tony Thottan (thotz)
